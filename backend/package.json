{
  "name": "e-commerce-shaky-server",
  "version": "1.0.0",
  "description": "An food supplement e-commerce web server",
  "scripts": {
    "build": "rimraf ./dist && tsc -p . && npm run copy-non-ts",
    "start": "cross-env MODE=PRODUCTION node --experimental-modules ./dist/index.js",
    "copy-non-ts": "copyfiles -f \"./src/templates/**.ejs\" ./dist/templates",
    "deleteAndSeed": "cross-env DELETE=true node --experimental-specifier-resolution=node --loader ts-node/esm ./src/seederScript.ts",
    "seed": "cross-env DELETE=false node --experimental-specifier-resolution=node --loader ts-node/esm ./src/seederScript.ts",
    "dev": "nodemon"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "./*": "./dist/**/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "repository": {
    "type": "github",
    "url": "null"
  },
  "keywords": [
    "e-commerce",
    "food",
    "supplement",
    "proteins",
    "backend"
  ],
  "author": "Anis aka \"Anissem\" Dimassi",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "es-dirname": "^0.1.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "flat": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "mongoose-autopopulate": "^0.16.1",
    "nodemailer": "^6.7.6",
    "readable-http-codes": "^1.1.1",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/flat": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.4",
    "@zoltu/typescript-transformer-append-js-extension": "^1.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.18",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
